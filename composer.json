{
    "name": "meridienclube/laravel-option",
    "version": "dev-master",
    "description": "Pacote de options para laravel",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Rafael Zingano",
            "email": "confrariaweb@gmail.com"
        }
    ],
    "require": {
        "laravelcollective/html": "^6",
        "meridienclube/laravel-select2": "dev-master",
        "meridienclube/laravel-vendor": "dev-master"
    },
    "autoload": {
        "files": [
            "src/Helpers/option.php",
            "src/Helpers/form_option.php",
            "src/Helpers/option_input.php"
        ],
        "psr-4": {
            "ConfrariaWeb\\Option\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConfrariaWeb\\Option\\Providers\\OptionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
